{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "max-class-per-file": false,
    "object-literal-sort-keys": false,
    "class-name": false,
    "interface-name": [false],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-trailing-underscore",
      "allow-pascal-case",
      "allow-snake-case"
    ],
    "prettier": true
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
